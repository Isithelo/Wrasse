 
<div class="card" style="width: 18rem;"> 
  <a href="/users/{{username}}">
    <img src="" id="{{ids}}_pic" class="img-fluid mr-1 userpicSingle " style="max-width: 100%">
  </a>
  <div class="card-body">
    <h5 class="card-title" id="{{ids}}_username"></h5>
    <p class="card-text" id="{{ids}}_bio"></p>
    <a href="/users/TestUser" target="_blank" class="btn btn-outline-success">View TestUser</a>
  </div>
</div>
<script type="text/javascript">
  $(document).ready(function () {
    var username = '{{username}}'
    var ids ='{{ids}}'
    userPicLoadSingle(username,ids)
  })
  
  function userPicLoadSingle(username,ids){
    jQuery.ajax({
      'url': '/profilepic/'+username,
      'async': true,
      'global': false,
      'success': function (dataSchemaObject) {
       $('#'+ids+'_pic').attr('src', JSON.parse(dataSchemaObject.image));
       $('#'+ids+'_username').html( dataSchemaObject.username);
       $('#'+ids+'_bio').html( dataSchemaObject.bio);
     }
   })
  }
</script>
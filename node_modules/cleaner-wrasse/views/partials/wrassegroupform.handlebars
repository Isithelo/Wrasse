                <!--Group issue form-->            
                <div class="container">
                  <div class="row">
                    <div class="col-xs-12 col-sm-4">
                      <div class="card" style="display: block;
                      margin-bottom: 20px;
                      line-height: 1.42857143;
                      background-color: #fff;
                      border-radius: 2px;
                      box-shadow: 0 2px 5px 0 rgba(0,0,0,0.16),0 2px 10px 0 rgba(0,0,0,0.12);
                      transition: box-shadow .25s;">
                      <img src="https://www.gravatar.com/avatar/kjusdbkjsjkfs" class="card-img-top" alt="..."  class="img-fluid"/>
                      <br />
                      <div class="card-content" style="padding:15px;
                      text-align:left;">
                      <h4 class="card-title" style="margin-top:0px;
                      font-weight: 700;
                      font-size: 1.65em; text-align:center;">
                      <form action="/issues/create"  method="post" autocomplete="off">
                        <div class="form-group">
                         <input type="text" name="issueGroup"  placeholder="Group title" value="{{wrassequery.issueGroup}}">
                       </div> 
                     </h4>
                     <div class="" style="text-align: left;">
                      All issues related to issue ?
                    </div>
                    <hr>
                    <div class="form-group">
                      <div class="input-icons">

                        <input type="text" class="form-control" id="title" name="title" placeholder="Issue title" value="{{wrassequery.title}}">
                      </div>
                    </div>
                    <div class="form-group">
                      <textarea class="form-control" placeholder="Desciption" id="description" name="description" rows="5" value="">{{wrassequery.description}}</textarea>
                    </div>
                    <div class="form-group" style="text-align: center;">
                      <button type="submit" href='/' role="button" class="btn btn-success">Submit new group</button>
                    </div> 
                  </form>
                </div>
              </div>
            </div>
          </div>
        </div>

<script type="text/javascript">
  function getviewall(location) {
    jQuery.ajax({
      'async': true,
      'global': false,
      'url': '/issues/ajaxview/',
      'success': function (dataSchemaObject) {
        jQuery('#'+location).html(dataSchemaObject)
      }
    });
  }
</script>